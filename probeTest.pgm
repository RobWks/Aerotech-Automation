DVAR $height
DVAR $total
DVAR $samples
DVAR $loop_var

$samples = 1000

MSGCLEAR -1 

WHILE True DO
	$total = 0
	FOR $loop_var = 1 TO $samples
		;$total = $total + ($AI0.0-0.886638)*(4.345413-0.886638)
		$total = $total + $AI0.0
	NEXT
	$height = $total/$samples
	MSGDISPLAY 1, $height
ENDWHILE